{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "es2017",
    "sourceMap": false,
    "incremental": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "baseUrl": "./"
  }
}
